-var titulo = "Titulo principal"
-var titulos = ["Titulo Principal", "Subtitulo 1", "Subtitulo 2", "Subtitulo 3"]
-var enlaces = ['Acerca', 'Noticias', 'contacto']
-var usuario = "Angel"
mixin caja(imagen,titulo,contenido)
	.caja
		.caja__imagen: img(src="./images/"+imagen, alt="Imagen vacia")
		.caja__conatenido
			h3=titulo
			p=contenido